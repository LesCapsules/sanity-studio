{
  "name": "sanity-studio",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "package.json",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc",
    "deploy": "npm run deploy:studio && npm run deploy:graphql",
    "deploy:studio": "sanity deploy",
    "deploy:graphql": "sanity graphql deploy --force",
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/locale-fr-fr": "^1.2.10",
    "@sanity/vision": "3.66.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.4.0",
    "sanity": "3.66.1",
    "sanity-plugin-media": "^2.0.2",
    "styled-components": "6.1.13",
    "typescript": "5.7.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@typescript-eslint/parser": "8.17.0",
    "eslint": "9.16.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "globals": "^15.12.0",
    "prettier": "3.4.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs",
      "github>browniebroke/renovate-configs:eslint-prettier"
    ]
  }
}
