{
  "name": "sanity-studio",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "package.json",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc",
    "deploy": "npm run deploy:studio && npm run deploy:graphql",
    "deploy:studio": "sanity deploy",
    "deploy:graphql": "sanity graphql deploy --force",
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/locale-fr-fr": "^1.2.10",
    "@sanity/vision": "5.6.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-icons": "5.5.0",
    "sanity": "5.6.0",
    "sanity-plugin-media": "^4.0.0",
    "styled-components": "6.3.8",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "globals": "^17.0.0",
    "prettier": "3.8.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs",
      "github>browniebroke/renovate-configs:eslint-prettier"
    ]
  }
}
