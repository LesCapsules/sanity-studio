{
  "name": "sanity-studio",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "package.json",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss|md)\"",
    "lint:eslint": "eslint --ext .js,.jsx .",
    "lint:prettier": "prettier --check \"**/*.+(js|jsx|json|css|scss|md)\"",
    "deploy": "yarn run deploy:studio && yarn run deploy:graphql",
    "deploy:studio": "sanity deploy",
    "deploy:graphql": "sanity graphql deploy --no-playground",
    "build": "sanity build",
    "start": "sanity start",
    "purge-assets": "sanity exec scripts/purge-unused-assets.js --with-user-token",
    "pretest": "yarn build",
    "test": "sanity check"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/base": "2.2.5",
    "@sanity/cli": "2.2.5",
    "@sanity/components": "2.1.4",
    "@sanity/core": "2.2.3",
    "@sanity/default-layout": "2.2.5",
    "@sanity/default-login": "2.1.4",
    "@sanity/desk-tool": "2.2.5",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "4.1.0",
    "sanity-plugin-media": "0.4.0"
  },
  "devDependencies": {
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "prettier": "2.2.1"
  }
}
